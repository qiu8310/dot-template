{
  "name": "dot-template-cli",
  "description": "create new files according to pre-defined templates",
  "version": "0.0.8",
  "license": "MIT",
  "scripts": {
    "prepublish": "npm run build",
    "clear": "remove-all out",
    "build": "npm run clear && tsc -p ./",
    "watch": "npm run clear && tsc -watch -p ./"
  },
  "bin": {
    "dtpl": "out/cli.js"
  },
  "keywords": [
    "dtpl",
    "template",
    "file",
    "variable"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/qiu8310/dot-template"
  },
  "devDependencies": {
    "@types/chokidar": "~1.7.3",
    "@types/fs-extra": "~5.0.0",
    "@types/inquirer": "0.0.37",
    "@types/node": "^8.9.0",
    "ts-node": "^4.1.0",
    "typescript": "^2.7.1"
  },
  "dependencies": {
    "chokidar": "~2.0.0",
    "dot-template-core": "^0.0.8",
    "fs-extra": "~5.0.0",
    "inquirer": "~5.1.0",
    "mora-scripts": "~1.6.20",
    "tslib": "^1.9.0"
  }
}
